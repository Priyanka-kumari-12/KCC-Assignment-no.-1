public class FloatArraySum {
    public static void main(String[] args) {
      
        float[] floatArray = { 1.5f, 2.0f, 3.7f, 4.2f, 5.1f };

        float sum = 0.0f;
        for (int i = 0; i < floatArray.length; i++) {
            sum += floatArray[i];
        }

        System.out.println("Sum of the elements in the array: " + sum);
    }
}




